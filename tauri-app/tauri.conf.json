{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": ".",
    "distDir": ".",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "GAINS",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.yourorg.gains",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "GAINS",
        "width": 800,
        "height": 600
      }
    ]
  }
} 